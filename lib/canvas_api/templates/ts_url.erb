<%= ts_type_creator(@nickname, @parameters) %>
export class <%=@nickname.camelize%>Action implements CanvasApiAction {
<% if @parameters.present? %>
  public readonly params: <%=@nickname.camelize%>InputType;
<% end %>

<% if @parameters.present? %>
  constructor(<%=@nickname.camelize%>Input: <%=@nickname.camelize%>InputType) {
  <% if @parameters.present? %>
    this.params = <%=@nickname.camelize%>Input;
  <% end %>
  }
<% end %>
  get uri():string {
    return <%=ts_url_parts(@api_url).join(' + ')%>;
  }
  get method():HttpMethod {
    return "<%=@method%>";
  }
}
